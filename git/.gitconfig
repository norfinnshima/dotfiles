[core]
	excludesfile = /Users/xxxxxxxxxx/.gitignore_global
	quotepath = false
[user]
	name = xxxxxxxxxxxxxxxxxxxx
	email = xxxxxxxxxx@xxxxxxxxxx
[init]
	defaultBranch = main
[alias]
	st = status
	adl = add --all
	cm = commit -m
	log = log --graph
	df = diff
	ch = checkout
	br = branch
[status]
	showUntrackedFiles = all
[merge]
	conflictStyle = zdiff3
[color]
	ui = auto
